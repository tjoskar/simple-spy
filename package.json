{
  "name": "simple-spy",
  "version": "3.0.1",
  "description": "yet another spy library",
  "license": "MIT",
  "repository": "tjoskar/simple-spy",
  "author": {
    "name": "Oskar Karlsson",
    "email": "kontakta@oskarkarlsson.nu",
    "url": "http://oskarkarlsson.nu"
  },
  "contributors": [
    "Shahar Or <mightyiampresence@gmail.com> (http://mightyi.am)"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "compile": "tsc",
    "unit": "nyc ava",
    "test": "run-s lint compile unit",
    "prepare": "npm test",
    "preversion": "npm run compile",
    "postversion": "git push; git push --tags"
  },
  "files": [
    "lib/index.js",
    "lib/index.d.ts"
  ],
  "keywords": [
    "spy"
  ],
  "typescript": {
    "definition": "./index.d.ts"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "ava": "3.5.0",
    "eslint": "6.8.0",
    "eslint-config-standard-with-typescript": "15.0.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "nyc-config-100": "1.0.1",
    "typescript": "3.8.3"
  },
  "nyc": {
    "extends": "nyc-config-100",
    "cache": false,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**",
      "**/*.test.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
